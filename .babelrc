{
	"presets": [
    ["env", {
      "targets": {
        "uglify": true
      },
      "modules": false
    }],
		"react"
	],
	"plugins": [
		"transform-class-properties",
		"transform-object-rest-spread",
    "transform-decorators-legacy"
  ],
	"env": {
	  "test": {
	    "presets": [
	      "env",
	      "react"
	    ],
      "plugins": [
        "system-import-transformer",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-decorators-legacy"
      ]
    }
  }
}
